{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "babel",
    "no-empty-blocks",
    "react"
  ],
  "globals": {
    "$": true,
    "jQuery": true,
    "React": true,
    "ReactDOM": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": 1,
    "eqeqeq": [2, "smart"],
    "brace-style": 2,
    "camelcase": [2, {"properties": "never"}],
    "comma-spacing": 2,
    "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": {"var":2, "let": 2, "const": 3} }],
    "key-spacing": 2,
    "linebreak-style": 2,
    "no-array-constructor": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 0,
    "no-empty": 0,
    "semi-spacing": 2,
    "semi": 2,
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-unary-ops": 2,
    "spaced-comment": 0,
    "arrow-spacing": 2,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "prefer-const": 2,
    "prefer-spread": 1,
    "require-yield": 1,
    "babel/generator-star-spacing": 2,
    "babel/object-shorthand": 2,
    "babel/arrow-parens": 2
  }
}
